{
  "version": 2,
  "builds": [
    {
      "src": "dist/index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/auth(.*)",
      "dest": "/index.html",
      "caseSensitive": false
    },
    {
      "src": "/dashboard(.*)",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html",
      "status": 200
    }
  ],
  "devCommand": "npm run dev"
}
